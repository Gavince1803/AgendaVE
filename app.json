{
  "expo": {
    "name": "MiCita",
    "slug": "AgendaVE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-final.png",
    "scheme": "agendave",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gavince1803.AgendaVE",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-final.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gavince1803.AgendaVE",
      "usesCleartextTraffic": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "Permite a AgendaVE acceder a tus fotos para actualizar tu perfil."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "69bc4e64-1cfc-420b-ad61-eb8d3df07f03"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/69bc4e64-1cfc-420b-ad61-eb8d3df07f03"
    }
  }
}
